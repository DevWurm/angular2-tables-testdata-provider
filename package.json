{
  "name": "angular2-tables-testdata-provider",
  "version": "1.0.0",
  "description": "Generator for test data for the angular2-tables evaluation app",
  "main": "src/index.js",
  "scripts": {
    "test": "mocha --recursive --ui tdd --compilers js:babel-core/register",
    "clean": "rm -rf lib",
    "build": "npm run clean && babel src -d lib",
    "build-dev": "npm run clean && babel src -d lib --source-maps \"both\"",
    "prepublish": "npm run build"
  },
  "keywords": [
    "DB",
    "data"
  ],
  "author": "DevWurm",
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-core": "^6.11.4",
    "babel-preset-es2015": "6.9.0",
    "chai": "3.5.0",
    "mocha": "2.5.3",
    "mongodb": "^2.2.4"
  },
  "license": "MIT",
  "dependencies": {
    "dateformat": "^1.0.12",
    "elasticsearch": "^11.0.1",
    "file-exists": "^2.0.0",
    "minimist": "^1.2.0"
  }
}
